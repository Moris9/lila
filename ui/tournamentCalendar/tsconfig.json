{
  "include": ["src/*.ts", "src/*.js"],
  "exclude": [],
  "compilerOptions": {
    "allowJs": true,
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": false,
    "moduleResolution": "node",
    "target": "es2016",
    "lib": ["DOM", "es2016"]
  }
}
