{
  "include": ["src/*.ts", "src/*.js"],
  "exclude": [],
  "compilerOptions": {
    "allowJs": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "target": "ES5",
    "lib": ["DOM", "ES5", "es2015.core", "es2015.Promise"]
  }
}
