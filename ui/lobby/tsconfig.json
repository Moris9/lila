{
  "include": ["src/*.ts", "src/*.js"],
  "exclude": [],
  "compilerOptions": {
    "allowJs": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "target": "es5",
    "lib": ["DOM", "ES5"]
  }
}
